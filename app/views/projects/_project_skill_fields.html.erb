<div class="col-md-12 project-skill" id="project_skill-<%=id%>">
<% byebug %>
	<%= f.input :id, as: :hidden %>
	<%= f.simple_fields_for :skill do |builder| %>
		<div class="col-md-3"><%= builder.input :name, label: "Skill", input_html: {onclick:"skill_autocomplete('#{id}')", id: "skill-name-#{id}"} %></div>
	<% end %>
	<div class="col-md-6"><%= f.input :question_id, required:true, collection: @questions, prompt: "Select Classification", label: "Associated Survey Skill and Level"%></div>
	<div class="col-md-1">
		<%= f.input :_destroy, as: :hidden, input_html: {id: "project-skill-destroy-#{id}"}%>
		<span class="glyphicon glyphicon-remove" onclick="remove_project_skill('<%=id%>')"></span>
	</div>
</div>